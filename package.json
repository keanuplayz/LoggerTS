{
  "name": "logger-ts",
  "version": "0.1.0",
  "description": "Discord message logger by Zeehondie rewritten to TypeScript.",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "once": "tsc && npm start",
    "dev": "tsc-watch --onSuccess \"node dist/index.js dev\"",
    "lint": "eslint src --ext .js,.ts --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keanuplayz/LoggerTS.git"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js"
  ],
  "author": "Keanu Timmermans",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/keanuplayz/LoggerTS/issues"
  },
  "homepage": "https://github.com/keanuplayz/LoggerTS#readme",
  "devDependencies": {
    "@types/node": "14.14.32",
    "@types/ws": "7.4.0",
    "@typescript-eslint/eslint-plugin": "4.17.0",
    "@typescript-eslint/parser": "4.17.0",
    "eslint": "7.21.0",
    "eslint-config-dmitmel": "github:dmitmel/eslint-config-dmitmel",
    "eslint-plugin-node": "11.1.0",
    "ts-node": "9.1.1",
    "tsc-watch": "4.2.9",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "discord.js": "^12.5.1",
    "discord.js-commando": "^0.12.2"
  }
}
